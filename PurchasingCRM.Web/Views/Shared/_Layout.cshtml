    <script src="@Url.Content("~/Content/js/daterangepicker.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/morris.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/raphael-min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.waypoints.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.counterup.min.js")" type="text/javascript"></script>
